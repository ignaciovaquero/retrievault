{
  "log_level": "debug",
  "log_file": "stderr",
  "secrets": {
    "/etc/certs": {
      "type": "certs",
      "vault_path": "pki/issue/es-datio",
      "parameters": {
        "common_name": "gocd.es.datio",
        "ttl": "24h",
        "alt_names": [
          "gocd"
        ],
        "ip_sans": [
          "10.142.242.119"
        ],
        "key": {
          "name": "gocd.key",
          "perm": "0600"
        },
        "cert": {
          "name": "gocd.crt",
          "perm": "0644"
        }
      }
    },
    "/etc/ssh": {
      "type": "generic",
      "vault_path": "gocd/github_ssh_keys",
      "parameters": {
        "private": {
          "name": "id_rsa_github",
          "perm": "0600"
        },
        "public": {
          "name": "id_rsa_github.pub",
          "perm": "644"
        }
      }
    }
  },
  "ca_cert_path": "/etc/certs/ca.crt",
  "insecure": false,
  "vault_addr": "https://vault.es.datio:8200",
  "vault_token": "token"
}
